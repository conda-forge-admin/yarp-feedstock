From 2a84af8cb3b29bf7a9fedbb030898a719d3c33dd Mon Sep 17 00:00:00 2001
From: Silvio Traversaro <silvio@traversaro.it>
Date: Fri, 20 Jan 2023 22:07:20 +0100
Subject: [PATCH] YarpSystemCheck: Does not overwrite the value of
 YARP_FLOAT<N>_IS_IEC559 variables

---
 cmake/YarpSystemCheck.cmake | 4 ----
 1 file changed, 4 deletions(-)

diff --git a/cmake/YarpSystemCheck.cmake b/cmake/YarpSystemCheck.cmake
index 47644c5849..e02331cad0 100644
--- a/cmake/YarpSystemCheck.cmake
+++ b/cmake/YarpSystemCheck.cmake
@@ -64,10 +64,6 @@ unset(YARP_FLOAT32)
 unset(YARP_FLOAT64)
 unset(YARP_FLOAT128)
 
-set(YARP_FLOAT32_IS_IEC559 0)
-set(YARP_FLOAT64_IS_IEC559 0)
-set(YARP_FLOAT128_IS_IEC559 0)
-
 set(YARP_HAS_FLOAT128_T 0)
 
 check_type_size("float" SIZEOF_FLOAT)
